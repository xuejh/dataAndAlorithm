package com.aa.bb;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		/*
		 * 0 1 1 2 3 5 8
		 */
		System.out.println(fib1(10));
		System.out.println(fib2(10));
	}
	
	
	public static int fib1(int n) {
		
		if(n<=1) return n;
		
		return fib1(n-1) + fib1(n - 2);
	}
	
	public static int fib2(int n) {
		if(n<=1) return n;
		int first = 0;
		int sec = 1;
		int sum = 0;
		for(int i=0;i<n-1;i++) {
			sum = first + sec;
			first = sec;
			sec = sum;
			
		}
		return sum;
	}
}
